  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach Host",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:5757/?ckcachecontrol=1526315733",
      "webRoot": "${workspaceFolder}/docs"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "file": "${workspaceFolder}/docs/index.html",
      "sourceMaps": false,
      "userDataDir": "/tmp/chrome-debug"
    },
    {
      "name": "Attach to url",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:5757/",
      "webRoot": "${workspaceFolder}/docs"
    }
  ]
}
